<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="enter_pwd">
        <item name="android:textSize">19.39sp</item>
        <item name="android:textColor">#000000</item>
        <item name="android:fontFamily">@font/lexend_medium</item>
    </style>
    <style name="KeyButton" parent="Widget.AppCompat.Button">
        <item name="android:layout_width">66dp</item>
        <item name="android:layout_height">32dp</item>
        <item name="android:background">@drawable/pwd_background_selector</item>
        <item name="android:textColor">@drawable/pwd_num_selector</item>
        <item name="android:fontFamily">@font/lexend_medium</item>
    </style>
    <style name="GotIt">
        <item name="android:background">@drawable/got_it_background_selector</item>
        <item name="android:textColor">@drawable/got_it_num_selector</item>
        <item name="android:fontFamily">@font/lexend_medium</item>
    </style>
    <style name="enter_pwd_num">
        <item name="android:background">@drawable/enter_pwd_background</item>
        <item name="android:textColor">@drawable/got_it_num_selector</item>
        <item name="android:fontFamily">@font/lexend_400</item>
    </style>
    <style name="robot_card">
        <item name="android:background">@drawable/robot_card_background</item>
    </style>
    <style name="robot_summon">
        <item name="android:background">@drawable/robot_summon_background</item>
        <item name="android:textColor">@color/default_summon</item>
        <item name="android:fontFamily">@font/lexend_medium</item>
    </style>
    <style name="heading_cancel">
        <item name="android:background">@drawable/heading_cancel_background</item>
        <item name="android:textColor">@color/default_summon</item>
        <item name="android:fontFamily">@font/lexend_medium</item>
    </style>
    <style name="point">
        <item name="android:background">@drawable/point_background_normal</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:fontFamily">@font/lexend_medium</item>
    </style>
    <style name="error_pwd_hint">
        <item name="android:background">@drawable/error_pwd_hint_background</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:fontFamily">@font/lexend_medium</item>
    </style>
</resources>